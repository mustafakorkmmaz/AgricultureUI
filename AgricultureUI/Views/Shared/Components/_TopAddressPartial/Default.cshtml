@model List<Address>
<div class="w3l_header_left">
	<ul>
		@foreach(var address in Model) 
		
		{
		<li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>@address.FDesc</li>
		<li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span> @address.FoDesc</li>
		<li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <a href="mailto:info@example.com">@address.SDesc</a></li>
		}
	</ul>

</div>
